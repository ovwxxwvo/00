#!/usr/bin/env dash


block="$HOME/.config/swaybar/block"
. "$block"/icon.bsh
. "$block"/color.bsh
LABEL="$ICON_screen"


show_block() {
  while true; do
    BLOCK="$LABEL"
    echo "$BLOCK"
    sleep 1
    break
  done
    BLOCK=""
    echo "$BLOCK"
  }

show_color() {
    echo $c_resk
  }


show_block
