# TMUX config, terminal multiplexer.
# Maintainer:	 Jessee Chan <CYPHER0@foxmail.com>
# Last Change: 2023.05.15


# basic
  unbind -n   C-v           # run -b ' tmux set-buffer "$(xclip -o -sel clipboard)" ; tmux paste-buffer '
  unbind -n   F1            # " split-window -h \; swap-pane -U "
  unbind -n  $c_escape      # " source ~/.config/tmux/key/redo.tmx \; display 'Reloaded Tmux Key' "   # C-Esc
  unbind -n  $c_tab         # copy-mode       # C-Tab

# tab & win
  unbind -n   C-Space       # selectp -R
  unbind -n   C-q           # kill-pane
  unbind -n  $c_m           # new-window      # C-m
  unbind -n  $c_bracketl    # splitw -h       # C-[
  unbind -n  $c_bracketr    # splitw -v       # C-]
  unbind -n  $c_apostrophe  # next-layout     # C-'

# tab focus
  unbind -n  $c_key1        # selectw -t 1    # C-1
  unbind -n  $c_key2        # selectw -t 2    # C-2
  unbind -n  $c_key3        # selectw -t 3    # C-3
  unbind -n  $c_key4        # selectw -t 4    # C-4
  unbind -n  $c_key5        # selectw -t 5    # C-5
  unbind -n  $c_key6        # selectw -t 6    # C-6
  unbind -n  $c_key7        # selectw -t 7    # C-7
  unbind -n  $c_key8        # selectw -t 8    # C-8
  unbind -n  $c_key9        # selectw -t 9    # C-9
  unbind -n  $c_key0        # selectw -t 10   # C-0
# tab focus
  unbind -n  $c_comma       # selectw -p      # C-,
  unbind -n  $c_period      # selectw -n      # C-.
# win focus
  unbind -n   C-j           # selectp -L      # C-j
  unbind -n   C-k           # selectp -U      # C-k
  unbind -n   C-l           # selectp -D      # C-l
  unbind -n  $c_semicolon   # selectp -R      # C-;

# win move out tab
# win move  in tab
  unbind -n   C-S-Left      # swapp -U
  unbind -n   C-S-Right     # swapp -D
  unbind -n   C-S-Up        # swapp -U
  unbind -n   C-S-Down      # swapp -D
# win resize
  unbind -n   C-Up          # resizep -U 2
  unbind -n   C-Down        # resizep -D 2
  unbind -n   C-Left        # resizep -L 2
  unbind -n   C-Right       # resizep -R 2


